# Telegram Bot Configuration
TELEGRAM_BOT_TOKEN=your_bot_token_here
TELEGRAM_API_URL=https://api.telegram.org/bot

# Webhook Security (optional)
WEBHOOK_SECRET=your_webhook_secret_here

# Search API Configuration (choose one)
# Яндекс.Поиск API (рекомендуется для РФ, не требует карту)
# YANDEX_API_KEY=your_yandex_api_key_here (IAM токен или API ключ из Yandex Cloud)
# YANDEX_FOLDER_ID=your_folder_id_here (ID каталога в Yandex Cloud, обязателен)
# YANDEX_AUTH_TYPE=Api-Key (опционально: Api-Key или Bearer, по умолчанию Api-Key)

# Google Custom Search API
# GOOGLE_API_KEY=your_google_api_key_here
# GOOGLE_SEARCH_ENGINE_ID=your_search_engine_id_here

# Bing Search API
# BING_API_KEY=your_bing_api_key_here

# SerpAPI
# SERPAPI_KEY=your_serpapi_key_here

# AI Service API Keys
# OpenAI API (required for AI comparison)
# Можно использовать либо прямой ключ OpenAI, либо ключ OpenRouter
OPENAI_API_KEY=your_openai_key_here
# Использовать OpenRouter вместо OpenAI (установите в true, если используете ключ OpenRouter)
USE_OPENROUTER=false
# Опционально для OpenRouter (для идентификации приложения)
# OPENROUTER_REFERER=https://findorigin.vercel.app
# OPENROUTER_TITLE=FindOrigin Bot
# ANTHROPIC_API_KEY=your_anthropic_key_here

